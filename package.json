{
  "name": "ph-todo-frontend",
  "version": "0.1.0",
  "description": "Frontend server for ph-todo-example.",
  "main": "index.js",
  "scripts": {
    "build": "npm run react:build ; npm run react:coffee",
    "provision": "node $PWD/build/server-assets/provision.js",
    "start": "node $PWD/build/server-assets/server.js",
    "react:start": "cd $PWD/src/react-client/ ; react-scripts start",
    "react:serve": "node src/server-assets/build/server.js",
    "react:coffee": "cd $PWD/src/server-assets/ ; coffee -o $PWD/build/ -c $PWD/src/",
    "react:build": "cd $PWD/src/react-client/ ; react-scripts build",
    "react:test": "cd $PWD/src/react-client/ ; react-scripts test --env=jsdom"
  },
  "author": "chippawah <charles.cantrell@me.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "angular": "^1.6.1",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-router": "^0.4.2",
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "pug-cli": "^1.0.0-alpha6",
    "q": "^1.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "seneca": "^3.2.2",
    "seneca-amqp-transport": "2.1.0",
    "seneca-legacy-logger": "^0.1.0",
    "tcp-ping": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "coffeescript": "^2.0.2"
  }
}
